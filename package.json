{
  "name": "contact-manager-typescript",
  "version": "1.0.0",
  "description": "Command-Line Contact Management utility built in Typescript",
  "main": "dist/contact.js",
  "preferGlobal": true,
  "bin": "dist/contact.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "start": "nodemon --exec ts-node -- contact.ts",
    "ts-node": "ts-node contact.ts",
    "watch": "tsc -w"
  },
  "keywords": [
    "contact",
    "typescript",
    "cli",
    "command",
    "ts"
  ],
  "author": "Luis Jesus Diaz Manzo",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "inquirer": "^7.3.3",
    "ora": "^4.1.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/core-js": "^2.5.4",
    "@types/dotenv": "^8.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^10.17.32",
    "@types/ora": "^3.2.0",
    "nodemon": "^2.0.4"
  }
}
